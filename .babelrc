{
    "presets": ["next/babel"],
    "plugins": [
        [
            "module-resolver",
            {
                "root": ["."],
                "alias": {
                    "@style": "./styles",
                    "@comps": "./components",
                    "@model": "./models",
                    "@route": "./routes"
                },
                "cwd": "babelrc"
            }
        ],
        [
            "wrap-in-js",
            {
                "extensions": ["css$", "scss$"]
            }
        ]
    ],
    "ignore": []
}
