{
    "presets": ["next/babel"],
    "plugins": [
        [
            "module-resolver",
            {
                "root": ["."],
                "alias": {
                    "@style": "./styles",
                    "@comps": "./components",
                    "@model": "./models",
                    "@route": "./routes",
                    "dva": "dva-no-router"
                },
                "cwd": "babelrc"
            }
        ],
        [
            "wrap-in-js",
            {
                "extensions": ["css$", "scss$"]
            }
        ]
        // [
        //     "import",
        //     {
        //         "libraryName": "antd",
        //         "style": "css" // or 'css'
        //     }
        // ]
    ],
    "ignore": []
}
